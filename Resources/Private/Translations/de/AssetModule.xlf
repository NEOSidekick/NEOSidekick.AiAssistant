<?xml version="1.0"?>
<xliff version="1.2" xmlns="urn:oasis:names:tc:xliff:document:1.2">
    <file original="" source-language="en" target-language="de" datatype="plaintext">
        <body>
            <trans-unit id="label">
                <source>Alternative Text Generator</source>
                <target xml:lang="de">Alternativer Text Generator</target>
            </trans-unit>
            <trans-unit id="error.configuration">
                <source>This module is not configured correctly. Please consult the documentation!</source>
                <target xml:lang="de">Dieses Modul ist nicht korrekt konfiguriert. Bitte konsultiere die Dokumentation!</target>
            </trans-unit>
            <trans-unit id="error.noApiKey">
                <source>This feature is not available in the free version of NEOSidekick. Sign up for a free trial at: https://neosidekick.com/en/pricing.</source>
                <target xml:lang="de" state="translated">Diese Funktion ist in der kostenlosen Version von NEOSidekick nicht verfügbar. Registrieren Sie sich für eine kostenlose Testversion unter: https://neosidekick.com/preise.</target>
            </trans-unit>
            <trans-unit id="saveAndNextPage" xml:space="preserve">
                <source>Save all and get next page</source>
                <target xml:lang="de" state="translated">Alle Speichern und nächste Seite</target>
            </trans-unit>
            <trans-unit id="persist">
                <source>Save</source>
                <target xml:lang="de">Speichern</target>
            </trans-unit>
            <trans-unit id="persisted">
                <source>Saved</source>
                <target xml:lang="de">Gespeichert</target>
            </trans-unit>
            <trans-unit id="persisting">
                <source>Saving...</source>
                <target xml:lang="de">Speichert...</target>
            </trans-unit>
            <trans-unit id="discard">
                <source>Discard</source>
                <target xml:lang="de">Verwerfen</target>
            </trans-unit>
            <trans-unit id="assetListItemLabel">
                <source>File »{0}«</source>
                <target xml:lang="de">Datei »{0}«</target>
            </trans-unit>
            <trans-unit id="generating">
                <source>Generating...</source>
                <target xml:lang="de">Wird generiert...</target>
            </trans-unit>
            <trans-unit id="listEmpty">
                <source>There are no assets with a missing alternative text!</source>
                <target xml:lang="de">Es gibt keine Medien mit fehlendem alternativem Text!</target>
            </trans-unit>
            <trans-unit id="loading">
                <source>Loading...</source>
                <target xml:lang="de">Lade...</target>
            </trans-unit>
            <trans-unit id="intro">
                <source><![CDATA[With this tool, you can create image descriptions and save them in the title or description field of the media asses. These descriptions are optimized as image alternative texts for SEO and accessibility. <a href="https://neosidekick.com/produkt/features/bildbeschreibungs-generator" target="_blank" style="text-decoration: underline;">Read the tutorial on how a developer can integrate them.</a>]]></source>
                <target xml:lang="de"><![CDATA[Mit diesem Tool kannst du Bildbeschreibungen erstellen und diese in das Feld Titel oder Beschreibung des Mediums speichern. Diese Beschreibungen sind als Bild-Alternativtexte für SEO und Barrierefreiheit optimiert. <a href="https://neosidekick.com/produkt/features/bildbeschreibungs-generator" target="_blank" style="text-decoration: underline;">Hier gehts zum Tutorial, wie ein Entwickler sie einbinden kann.</a>]]></target>
            </trans-unit>
            <trans-unit id="configuration.onlyAssetsInUse.label">
                <source>Selection of images</source>
                <target xml:lang="de">Auswahl der Bilder</target>
            </trans-unit>
            <trans-unit id="configuration.onlyAssetsInUse.0">
                <source>All images</source>
                <target xml:lang="de">Alle Bilder</target>
            </trans-unit>
            <trans-unit id="configuration.onlyAssetsInUse.1">
                <source>Only images in use</source>
                <target>Nur Bilder in Verwendung</target>
            </trans-unit>
            <trans-unit id="configuration.propertyName.label">
                <source>Field to save the description in</source>
                <target xml:lang="de">Beschreibung speichern in</target>
            </trans-unit>
            <trans-unit id="configuration.limit.label">
                <source>Number of entries per page</source>
                <target xml:lang="de">Anzahl der Elemente pro Seite</target>
            </trans-unit>
            <trans-unit id="configuration.language.label">
                <source>Language for the description</source>
                <target xml:lang="de">Sprache der Beschreibung</target>
            </trans-unit>
            <trans-unit id="configuration.language.en">
                <source>English</source>
                <target xml:lang="de">Englisch</target>
            </trans-unit>
            <trans-unit id="configuration.language.de">
                <source>German</source>
                <target xml:lang="de">Deutsch</target>
            </trans-unit>
            <trans-unit id="configuration.language.fr">
                <source>French</source>
                <target xml:lang="de">Französisch</target>
            </trans-unit>
            <trans-unit id="configuration.language.es">
                <source>Spanish</source>
                <target xml:lang="de">Spanisch</target>
            </trans-unit>
            <trans-unit id="configuration.language.it">
                <source>Italian</source>
                <target xml:lang="de">Italienisch</target>
            </trans-unit>
            <trans-unit id="startModule">
                <source>Start generation</source>
                <target xml:lang="de">Generierung starten</target>
            </trans-unit>
        </body>
    </file>
