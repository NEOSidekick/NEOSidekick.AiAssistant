            </trans-unit>
            <trans-unit id="configuration.seoPropertiesFilter.only-empty-seo-titles">
                <source>Only without seo title</source>
                <target xml:lang="fr">Seulement sans titre SEO</target>
            </trans-unit>
            <trans-unit id="configuration.seoPropertiesFilter.only-empty-meta-descriptions">
