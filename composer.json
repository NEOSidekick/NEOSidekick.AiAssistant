{
    "name": "neosidekick/ai-assistant",
    "description": "Use the power of GPT-4 to assist you in writing content",
    "type": "neos-plugin",
    "license": "GPL-3.0-or-later",
    "keywords": [
        "flow",
        "neos",
        "openai",
        "gpt",
        "gpt-4",
        "chatgpt",
        "ai",
        "writing"
    ],
    "require": {
        "neos/neos": "~5.0 || ~7.0 || ~8.0 || dev-master"
    },
    "authors": [
        {
            "name": "Roland Sch√ºtz",
            "email": "rs@codeq.at",
            "homepage": "https://neosidekick.com/",
            "role": "Developer"
        }
    ],
    "archive": {
        "exclude": [
            ".editorconfig",
            ".gitattributes",
            ".github",
            ".gitignore"
        ]
    },
    "autoload": {
        "psr-4": {
            "NEOSidekick\\AiAssistant\\": "Classes"
        }
    },
    "extra": {
        "neos": {
            "package-key": "NEOSidekick.AiAssistant"
        },
        "applied-flow-migrations": []
    }
}
