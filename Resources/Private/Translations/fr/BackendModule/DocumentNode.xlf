                <target xml:lang="fr">Tous les types de pages</target>
            </trans-unit>
            <trans-unit id="configuration.languageDimensionFilter.label">
                <source>Select languages</source>
                <target xml:lang="fr">Sélectionner les langues</target>
            </trans-unit>
            <trans-unit id="configuration.languageDimensionFilter.all">
                <source>All languages</source>
